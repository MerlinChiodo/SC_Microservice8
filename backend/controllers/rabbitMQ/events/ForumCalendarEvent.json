{
    "title": "event_Calendar",
    "description": "Schema for the events that are appointed",
    "type": "object",
    "properties": {
        "event_id" : {
            "type": "integer",
            "minimum": 0,
            "maximum": 10000
        },
        "event_name": {
            "type": "string",
            "const": "newServicePost"
        },
        "title": {
            "type": "string"
        },
        "short_description": {
            "type": "string"
        },
        "long_description": {
            "type": "string"
        },
        "service": {
            "type": "string"
        },
        "event_on": {
            "type": "string"
        },
        "picture_url": {
            "type": "string",
            "minLength": 1
        }
    },
    "required": [
        "event_id",
        "event_name",
        "title",
        "short_description",
        "service",
        "event_on"
    ],
    "additionalProperties": false
}